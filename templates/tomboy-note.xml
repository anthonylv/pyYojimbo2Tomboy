<note version="0.3" xmlns:link="http://beatniksoftware.com/tomboy/link" xmlns:size="http://beatniksoftware.com/tomboy/size" xmlns="http://beatniksoftware.com/tomboy">
  <title>{{ title }}</title>
  <text xml:space="preserve"><note-content version="0.1">{{ note_body }}
</note-content></text>
  <last-change-date>{{ last_change_date }}</last-change-date>
  <last-metadata-change-date>{{ last_metadata_change_date }}</last-metadata-change-date>
  <create-date>{{ create_date }}</create-date>
  <cursor-position>27</cursor-position>
  <width>450</width>
  <height>360</height>
  <x>0</x>
  <y>0</y>
  <open-on-startup>False</open-on-startup>
</note>